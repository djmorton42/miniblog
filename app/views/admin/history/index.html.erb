<div class="row">
  <h2>Historical Entries for '<em><%= @entry.title %>'</em></h2>
  <h3>Last Updated: <em><%= format_datetime(@entry.updated_at) %></em></h3>
</div>
<div class="row">
  <table>
  <tr>
    <th>Title</th><th>Created Date</th><th>Actions</th>
  </tr>
  <%= render partial: "historical_entry_line_item", collection: @historical_entries, locals: { entry: @entry } %>
  </table>
</div>
<div class="row">
  <%= link_to('Back', admin_entries_path, class: 'button round') %>
</div>
