<tr>
  <td>
    <%= image_table_row.title %>
  </td>
  <td>
    <%= image_table_row.published_at.presence || 'Unpublished'  %>
  </td>
  <td>
    <%= image_table_row.created_at %>
  </td>
  <td>
    <% if image_table_row.is_published? %>
      <%= link_to(image_tag('ic_visibility_off_black_24dp.png'), "#{admin_image_path(image_table_row)}/unpublish", method: 'post') %>
    <% else %>
      <%= link_to(image_tag('ic_visibility_black_24dp.png'), "#{admin_image_path(image_table_row)}/publish", method: 'post') %>
    <% end %>
    <%= link_to(image_tag('ic_delete_black_24dp.png'), admin_image_path(image_table_row), method: 'delete') %>
  </td>
</tr>
